    "@lexical/link": "^0.12.5",
    "@lexical/list": "^0.12.5",
    "@lexical/rich-text": "^0.12.5",
    "@lexical/table": "^0.12.5",
    "@lexical/utils": "^0.12.5",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@vinejs/vine": "^1.7.0",
    "@vueuse/core": "^10.7.0",
    "@xata.io/client": "^0.28.1",
    "electron-trpc": "^0.5.2",
    "electron-updater": "^6.1.7",
    "emittery": "^1.0.1",
    "klona": "^2.0.6",
    "lexical": "^0.12.5",
    "lexical-vue": "^0.7.2",
    "lucide-vue-next": "^0.298.0",
    "naive-ui": "^2.35.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.44.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^1.0.29",
    "eslint": "^8.56.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
