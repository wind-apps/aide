{
  "name": "aide",
  "version": "1.0.0",
  "description": "A helpful bookmarking and knowledge base app.",
  "author": "Travis Reynolds <travis@travisreynolds.dev>",
  "homepage": "github:wind-apps/aide",
  "repository": "https://github.com/wind-apps/aide",
  "main": "./out/main/index.js",
  "volta": {
    "node": "18.19.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "generate:xata": "xata pull main",
    "generate": "pnpm run \"/^generate:.*/\"",
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run --parallel \"/^typecheck:.*/\"",
    "build": "pnpm run typecheck && electron-vite build",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:mac": "pnpm run build && electron-builder --mac --config",
    "build:linux": "pnpm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@fluent/bundle": "^0.18.0",
    "@fontsource-variable/fira-code": "^5.0.16",
    "@fontsource-variable/lexend": "^5.0.18",
    "@tiptap/extension-code-block-lowlight": "^2.1.15",
    "@tiptap/pm": "^2.1.15",
    "@tiptap/starter-kit": "^2.1.15",
    "@tiptap/vue-3": "^2.1.15",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@vinejs/vine": "^1.7.0",
    "@vueuse/core": "^10.7.1",
    "@xata.io/client": "^0.28.3",
    "dayjs": "^1.11.10",
    "electron-trpc": "^0.5.2",
    "electron-updater": "^6.1.7",
    "emittery": "^1.0.1",
    "fluent-vue": "^3.5.0",
    "klona": "^2.0.6",
    "lowlight": "^3.1.0",
    "lucide-vue-next": "^0.309.0",
    "luxon": "^3.4.4",
    "naive-ui": "^2.37.3",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/luxon": "^3.4.0",
    "@types/node": "^18.19.6",
    "@vitejs/plugin-vue": "^5.0.3",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "@xata.io/cli": "^0.15.4",
    "electron": "^27.2.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "2.0.0-beta.2",
    "eslint": "^8.56.0",
    "rollup-plugin-fluent-vue": "^3.0.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.11",
    "vite-plugin-windicss": "^1.9.3",
    "vue": "^3.4.7",
    "vue-tsc": "^1.8.27",
    "windicss": "^3.5.6"
  }
}
