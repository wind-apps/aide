{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "./src/renderer/src/*"
      ],
      "@main/*": [
        "./src/main/*"
      ]
    },
    "skipDefaultLibCheck": true
  },
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*.ts",
    "src/renderer/src/**/*.vue",
    "src/preload/*.d.ts"
  ]
}
